<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Redux"
                   namespace="Redux.Database.Models">

  <class name="EventReward" table="event_reward">
    <id name="Id" column="id">
      <generator class="identity" />
    </id>
    <property name="EventEntryId" column="event_entry_id" />
    <property name="RewardType" column="reward_type" />
    <property name="RewardValue" column="reward_value" />
    <property name="GrantedAt" column="granted_at" />
    <property name="Delivered" column="delivered" />
    <property name="DeliveredAt" column="delivered_at" />
  </class>

</hibernate-mapping>
